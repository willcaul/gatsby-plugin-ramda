{
  "name": "gatsby-plugin-ramda",
  "version": "1.0.1",
  "description": "Easy modular Ramda builds. Adds the Ramda Babel plugin to your Gatsby build",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willcaul/gatsby-plugin-ramda.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "ramda"
  ],
  "author": "Will Caulfield <willcaul@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willcaul/gatsby-plugin-ramda/issues"
  },
  "homepage": "https://github.com/willcaul/gatsby-plugin-ramda#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "babel-plugin-ramda": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.1.4"
  },
  "peerDependencies": {
    "gatsby": ">=2.0.0",
    "ramda": ">=0.25.0"
  }
}
